{
  "esnext": true,
  "preset": "airbnb",
  "excludeFiles": [
    "dist/**"
  ],
  "disallowKeywordsOnNewLine": [],
  "maximumLineLength": 128,
  "requireCurlyBraces": true,
  "requirePaddingNewlinesInBlocks": {
    "close": false,
    "open": true,
    "allExcept": [
      "conditionals"
    ]
  },
  "requireSpacesInAnonymousFunctionExpression": {
    "beforeOpeningCurlyBrace": true
  },
  "requireTrailingComma": false,
  "safeContextKeyword": [
    "_this",
    "self"
  ],
}
