{
  "name": "kryptnostic-js",
  "version": "1.0.0",
  "homepage": "https://github.com/kryptnostic/kryptnostic-js",
  "description": "JavaScript SDK for storing, sharing, and searching FHE-encrypted data.",
  "author": {
    "name": "Kryptnostic, Inc.",
    "email": "info@kryptnostic.com",
    "url": "https://www.kryptnostic.com"
  },
  "contributors": [
    {
      "name": "Hristo Oskov",
      "email": "hristo@kryptnostic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kryptnostic/kryptnostic-js.git"
  },
  "bugs": {
    "url": "https://github.com/kryptnostic/kryptnostic-js/issues"
  },
  "license": "Apache-2.0",
  "main": "dist/kryptnostic.js",
  "scripts": {
    "build": "npm-run-all forge:install --parallel build:dev build:prod",
    "build:dev": "npm run webpack:dev",
    "build:prod": "npm-run-all lint webpack:prod",
    "forge:install": "cd node_modules/node-forge && npm install && npm run minify && npm run bundle",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint --config .eslintrc src test",
    "test": "BUILD_ENV=TEST npm-run-all --parallel test:kjs test:krypto",
    "test:kjs": "karma start test/kjs.karma.config.js",
    "test:krypto": "karma start test/krypto.karma.config.js",
    "test:karma:run": "karma run test/kjs.karma.config.js --",
    "test:karma:start": "karma start test/kjs.karma.config.js --singleRun=false",
    "test:karma:watch": "karma start test/kjs.karma.config.js --singleRun=false --autoWatch=true",
    "test:karma:browser": "karma start test/kjs.karma.config.js --singleRun=false --autoWatch=true --browsers=Chrome --reporters=kjhtml",
    "webpack:dev": "BUILD_ENV=DEV webpack --config webpack.config.js",
    "webpack:dev:watch": "BUILD_ENV=DEV webpack --config webpack.config.js --watch",
    "webpack:prod": "BUILD_ENV=PROD webpack --config webpack.config.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.0",
    "krypto-js": "0.0.13",
    "node-forge": "^0.6.39"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0-beta.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-class-properties": "^6.5.0",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.0",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.6.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.3.4",
    "eslint": "^2.10.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-jasmine": "^1.6.0",
    "exports-loader": "^0.6.3",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-jasmine-diff-reporter": "^0.3.4",
    "karma-jasmine-html-reporter": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.25",
    "karma-webpack": "^1.7.0",
    "npm-run-all": "^1.5.2",
    "phantomjs-prebuilt": "^2.1.6",
    "webpack": "^2.1.0-beta"
  }
}
