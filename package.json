{
  "name": "kryptnostic-js",
  "version": "1.0.0",
  "homepage": "https://github.com/kryptnostic/kryptnostic-js",
  "description": "JavaScript SDK for storing, sharing, and searching FHE-encrypted data.",
  "author": {
    "name": "Kryptnostic, Inc.",
    "email": "info@kryptnostic.com",
    "url": "https://www.kryptnostic.com"
  },
  "contributors": [
    {
      "name": "Hristo Oskov",
      "email": "hristo@kryptnostic.com"
    },
    {
      "name": "Nick Hewitt",
      "email": "nick@kryptnostic.com"
    },
    {
      "name": "Stephanie A. Higa",
      "email": "stephanie@kryptnostic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kryptnostic/kryptnostic-js.git"
  },
  "bugs": {
    "url": "https://github.com/kryptnostic/kryptnostic-js/issues"
  },
  "license": "Apache-2.0",
  "main": "dist/kryptnostic.js",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run webpack:dev",
    "build:prod": "npm-run-all lint webpack:prod",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint --config .eslintrc src test",
    "lint:jscs": "jscs --config .jscsrc src test",
    "test": "NODE_ENV=TEST karma start test/karma.config.js",
    "test:karma:run": "karma run test/karma.config.js --",
    "test:karma:start": "karma start test/karma.config.js --singleRun=false",
    "test:karma:watch": "karma start test/karma.config.js --singleRun=false --autoWatch=true",
    "test:karma:browser": "karma start test/karma.config.js --singleRun=false --autoWatch=true --browsers=Chrome --reporters=kjhtml",
    "webpack:dev": "NODE_ENV=DEV webpack --config webpack.config.js",
    "webpack:dev:watch": "NODE_ENV=DEV webpack --config webpack.config.js --watch",
    "webpack:prod": "NODE_ENV=PROD webpack --config webpack.config.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.0",
    "krypto-js": "0.0.13"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0-beta.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-class-properties": "^6.5.0",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.0",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.6.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.3.4",
    "eslint": "^2.5.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-jasmine": "^1.6.0",
    "exports-loader": "^0.6.3",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jscs": "^2.11.0",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-jasmine-diff-reporter": "^0.3.4",
    "karma-jasmine-html-reporter": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.25",
    "karma-webpack": "^1.7.0",
    "npm-run-all": "^1.5.2",
    "phantomjs-prebuilt": "^2.1.6",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
